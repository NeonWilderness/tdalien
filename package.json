{
  "name": "alienlayout",
  "version": "1.4.0",
  "description": "Twoday Alien Template to host a foreign Wordpress site",
  "main": "alien.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/NeonWilderness/tdalien.git"
  },
  "scripts": {
    "build": "webpack --mode production",
    "deploy": "node ./utils/uploadSkin --blog=foundation",
    "deploy:ts": "node ./utils/uploadSkin --blog=talkstraight",
    "deploy:client": "node ./utils/uploadSkin --blog=spiegelei",
    "dev": "webpack --mode development",
    "patch": "node ./utils/bump --mode=patch",
    "minor": "node ./utils/bump --mode=minor",
    "major": "node ./utils/bump --mode=major",
    "start": "npm run build && npm run deploy"
  },
  "author": "NeonWilderness",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@types/cheerio": "^0.22.10",
    "@types/jquery": "^3.3.29",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^2.1.0",
    "dotenv-safe": "^6.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jshint": "^2.10.1",
    "jshint-loader": "^0.8.4",
    "jshint-stylish": "^2.2.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "semver": "^5.6.0",
    "slugify": "^1.3.4",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "url-join": "^4.0.0",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "yargs": "^13.2.1"
  }
}
