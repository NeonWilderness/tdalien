{
  "name": "alienlayout",
  "version": "1.9.0",
  "description": "Twoday Alien Template to host a foreign Wordpress site",
  "main": "alien.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/NeonWilderness/tdalien.git"
  },
  "scripts": {
    "build": "webpack --mode production",
    "deploy": "node ./utils/uploadSkin --blog=foundation",
    "deploy:client": "node ./utils/uploadSkin --blog=nberlin",
    "dev": "webpack --mode development",
    "patch": "node ./utils/bump --mode=patch",
    "minor": "node ./utils/bump --mode=minor",
    "major": "node ./utils/bump --mode=major",
    "release:download": "node ./utils/downloadLayout",
    "release:init": "node ./utils/uploadInit",
    "release": "npm run release:init && npm run release:download",
    "start": "npm run build && npm run deploy"
  },
  "author": "NeonWilderness",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@types/cheerio": "^0.22.18",
    "@types/jquery": "^3.3.38",
    "@types/request": "^2.48.5",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^3.6.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "less": "^3.11.3",
    "less-loader": "^6.1.1",
    "mini-css-extract-plugin": "^0.9.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "semver": "^7.3.2",
    "string-replace-loader": "^2.3.0",
    "style-loader": "^1.2.1",
    "url-join": "^4.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "yargs": "^15.3.1"
  }
}
