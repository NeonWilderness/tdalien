  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/jquery": "^3.3.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^0.28.11",
    "dotenv-safe": "^6.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jshint": "^2.9.5",
    "jshint-loader": "^0.8.4",
    "jshint-stylish": "^2.2.1",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "semver": "^5.5.0",
    "slugify": "^1.3.0",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.21.0",
    "url-join": "^4.0.0",
