{
  "name": "alienlayout",
  "description": "Twoday Alien Template to host a foreign Wordpress site",
  "version": "1.10.1",
  "author": "NeonWilderness",
  "bugs": {
    "url": "https://github.com/NeonWilderness/tdalien/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/cheerio": "^0.22.23",
    "@types/jquery": "^3.5.5",
    "@types/request": "^2.48.5",
    "babel-loader": "^8.2.2",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^5.0.1",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "less": "^4.0.0",
    "less-loader": "^7.1.0",
    "mini-css-extract-plugin": "^1.3.3",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "semver": "^7.3.4",
    "string-replace-loader": "^3.0.1",
    "style-loader": "^2.0.0",
    "url-join": "^4.0.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "yargs": "^16.2.0"
  },
  "homepage": "https://github.com/NeonWilderness/tdalien/blob/master/README.md",
  "keywords": [
    "Blog",
    "RSS",
    "Twoday",
    "iframe"
  ],
  "license": "MIT",
  "main": "alien.html",
  "repository": {
    "type": "git",
    "url": "git://github.com/NeonWilderness/tdalien.git"
  },
  "scripts": {
    "build": "webpack --mode production",
    "deploy": "node ./utils/uploadSkin --blog=foundation",
    "deploy:client": "node ./utils/uploadSkin --blog=xxxxxx",
    "dev": "webpack --mode development",
    "major": "node ./utils/bump --mode=major",
    "minor": "node ./utils/bump --mode=minor",
    "patch": "node ./utils/bump --mode=patch",
    "release": "npm run release:init && npm run release:download",
    "release:download": "node ./utils/downloadLayout",
    "release:init": "node ./utils/uploadInit",
    "status": "node ./utils/userStatus",
    "start": "npm run build && npm run deploy"
  }
}
